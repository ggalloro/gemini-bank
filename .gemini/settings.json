{
  "model": {
    "chatCompression": {
      "contextPercentageThreshold": 0.6
    }
  },
  "general": {
    "checkpointing": {
      "enabled": true
    }
  },
  "context": {
    "fileFiltering": {
      "respectGitIgnore": true
    }
  },
  "mcpServers": {
    "playwright": {
      "command": "npx",
      "args": [
        "@playwright/mcp@latest",
        "--headless",
        "--browser=chrome",
        "--viewport-size=1920, 1080"
      ]
    },
    "nano-banana": {
      "command": "mcp-gemini-go",
      "env": {
        "MCP_SERVER_REQUEST_TIMEOUT": "55000",
        "PROJECT_ID": "strebel-workspace",
        "GOPATH": "/home/user/go"
      }
    }
  },
  "privacy": {
    "usageStatisticsEnabled": true
  }
}