{
  "model": {
    "chatCompression": {
      "contextPercentageThreshold": 0.6
    }
  },
  "general": {
    "checkpointing": {
      "enabled": true
    }
  },
  "context": {
    "fileFiltering": {
      "respectGitIgnore": true
    }
  },
  "mcpServers": {
    "nano-banana": {
      "command": "/home/user/go/bin/mcp-gemini-go",
      "env": {
        "MCP_SERVER_REQUEST_TIMEOUT": "55000",
        "PROJECT_ID": "strebel-workspace",
        "GOPATH": "/home/user/go"
      }
    },
    "chrome-devtools": {
      "command": "npx",
      "args": [
        "chrome-devtools-mcp@latest",
        "--headless=true",
        "--viewport=1920x1080"
      ]
    }
  },
  "privacy": {
    "usageStatisticsEnabled": true
  }
}